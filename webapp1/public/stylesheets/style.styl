body
  padding: 50px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
  color: white
  background-color: #26343F
a
  color: #00B7FF

.table5
  border-collapse: collapse
  width: 800px
  th 
    background-color: #76747F
  th
    text-align: center

.menu-container
  width:960px
  height:50px


ul
  list-style-type: none
  margin: 0
  padding: 0
  color: #fff
  &#menu
    li
      float: left
      position: relative
      margin: 0 0 0 1px
      padding: 5px
      width: 160px
      background-color: #555555
      display: block
  &.child
    display: none
    position: absolute
    margin-left: -5px
    padding: 0;
    li a
      padding: 5px 10px
      margin-left: -5px
      margin-right: -5px
      margin-bottom: -5px
      display: block
      color: #fff

li
  a
    &:hover
      background: #ff0000
